<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycode.ticketbookingapp">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".TicketBookingApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:resizeableActivity="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TicketBookingApp">
        <activity
            android:name=".homefragment.bookingticketpage1.BookingTicketPage1Activity"
            android:exported="false"
            android:label="@string/title_activity_booking_ticket_page1"
            android:theme="@style/Theme.TicketBookingApp.NoActionBar">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".Launcher"
            android:exported="true"
            android:theme="@style/Launch">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".HomePage"
            android:exported="true"
            android:theme="@style/Theme.TicketBookingApp" />
        <activity
            android:name=".profilefragment.editprofile.EditProfile"
            android:exported="true" />
        <activity
            android:name=".profilefragment.settingsfragment.SettingsActivity"
            android:exported="true" />
        <activity
            android:name=".profilefragment.settingsfragment.Feedback"
            android:exported="true" />
        <activity
            android:name=".homefragment.gridview.GridViewActivity"
            android:exported="true" />
        <activity
            android:name=".homefragment.moviedescription.MovieDescriptionActivity"
            android:exported="true" />
        <activity
            android:name=".homefragment.bookingticketpage1.BookingTicketPage1Activity"
            android:exported="true" />
    </application>

</manifest>